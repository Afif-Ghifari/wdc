---
import "../static/scss/component.scss";
import "../static/scss/budaya.scss";
import MainLayout from "../layouts/main-layout.astro";
import BudayaPage from "../layouts/budaya.vue";
---

<MainLayout
  description="Temukan sejarah, tradisi, dan keunikan seni yang melekat dalam kehidupan masyarakat setempat."
  title="Budaya"
>
  <BudayaPage />
</MainLayout>
<script type="text/javascript">
document.addEventListener("DOMContentLoaded", function () {
  new Splide(".splide__1", {
    drag: "free",
    perPage: 2,
    type: "loop",
    autoScroll: {
      speed: 1,
    },
  }).mount(window.splide.Extensions);
});
</script>